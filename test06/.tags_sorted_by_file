email	sql.php	/^	$email = filter_input(INPUT_GET,'email');$/;"	v
key	sql.php	/^	foreach ($results as $key => $result) {$/;"	v
results	sql.php	/^	$results = $statement->fetchAll(PDO::FETCH_ASSOC);$/;"	v
sql	sql.php	/^	$sql = 'SELECT id FROM users WHERE email= :email';#:email是占位符，为了达到预处理的效果$/;"	v
statement	sql.php	/^	$statement = $pdo->prepare($sql);$/;"	v
